<h1>Posts</h1><%= link_to " (+)", new_post_path %>
<% @posts.each do |post| %>
  <h2><%= link_to post.title, post_path(post) %></h2>
  <h3><%= link_to post.user.username, user_path(post.user)%></h3>
  <h3><%=post.created_at.strftime("%b %e, %Y - %l:%M%p")%></h3>
  <p><%=post.post_content%></p>
<% end %>
